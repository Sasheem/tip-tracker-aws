{
  "expo": {
    "name": "tip-tracker-aws",
    "slug": "tip-tracker-aws",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#06D6A0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Tip tracking app useful for users working in the hospitality industry. Helps users keep track of earnings, wages, and hours worked over time. Calculates useful metrics for user based on their input.",
    "githubUrl": "https://github.com/Sasheem/tip-tracker-aws"
  }
}
