{
  "expo": {
    "name": "tip-tracker-aws",
    "slug": "tip-tracker-aws",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#39A0ED"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Track tips and wages earned working in the hospitality industry. Users can view averages and statistics over time about the data they submit. ",
    "githubUrl": "https://github.com/Sasheem/tip-tracker-aws"
  }
}
